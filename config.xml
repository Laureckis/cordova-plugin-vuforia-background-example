<?xml version='1.0' encoding='utf-8'?>
<widget id="lv.aspired.vuforia.example" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Cordova Vuforia Background Plugin Example</name>
    <description>
        A sample app showing how to use Cordova Vuforia Background plugin.
    </description>
    <author email="it@aspired.lv" href="http://aspired.lv">
        Aspired Interactived Ltd.
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <plugin name="cordova-plugin-vuforia-background" spec="https://github.com/Laureckis/cordova-plugin-vuforia-background.git" />
</widget>
